    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./build",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      }
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules", "**/*.test.ts"]
  }